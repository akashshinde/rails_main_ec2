
<%= simple_form_for @hk do |f| %>
  <%= f.input :name %>
  <%= f.input :hall_size %>
  <%= f.input :kitchen_size %>
  <%= f.input :floor, :collection => ["Ground","First","Second","Third","Fourth"] %>
  <%= f.input :sold_out %>
  <%= f.association :project, :include_blank => false %>
  <%= f.input :status %>
  <%= f.button :submit, :class => 'btn-primary' %>
    
<% end %>



